{
    "name": "projek-xyz/psr-container",
    "description": "PSR Container Implementation",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Fery Wardiyanto",
            "email": "ferywardiyanto@gmail.com"
        }
    ],
    "scripts": {
        "test": [
            "kahlan --config=test/config.php --coverage=3"
        ]
    },
    "autoload": {
        "psr-4": {
            "Projek\\": "src"
        }
    },
    "require": {
        "php": "^7.1",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "kahlan/kahlan": "^4.7"
    },
    "provide": {
        "psr/container-implementation": "^1.0"
    },
    "config": {
      "preferred-install": "dist",
      "sort-packages": true,
      "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
